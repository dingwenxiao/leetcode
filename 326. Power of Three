public boolean isPowerOfThree(int n) {
    return n > 0 && (1162261467 % n == 0);
}

public boolean isPowerOfThree(int n) {
        if(n==0) return false;
            double res = Math.log(n)/Math.log(3);
            int resInt = (int)Math.rint(res);
            return Math.abs(res-resInt)<=10e-15;
    }